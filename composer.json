{
    "name": "ronanchilvers/certificate-checker",
    "description": "Simple certificate checker",
    "type": "project",
    "require": {
        "slim/slim": "^3.9",
        "monolog/monolog": "^1.23",
        "vlucas/phpdotenv": "^2.4",
        "slim/twig-view": "^2.3",
        "ronanchilvers/sassy": "dev-master",
        "robmorgan/phinx": "^0.9.2",
        "ronanchilvers/container": "dev-master"
    },
    "authors": [
        {
            "name": "Ronan Chilvers",
            "email": "ronan@d3r.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "src"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "serve": "/usr/bin/env php -S localhost:8001 -t web"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitlab.com/ronanchilvers/sassy.git"
        },
        {
            "type": "vcs",
            "url": "https://gitlab.com/ronanchilvers/container.git"
        }
    ]
}
